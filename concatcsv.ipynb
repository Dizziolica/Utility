   "source": [
    "files = [i for i in os.listdir() if \".csv\" in i]\n",
    "df = pd.DataFrame()\n",
    "for files in files:\n",
    "    dfTemp = pd.read_csv(files)\n",
    "    df = pd.concat([df, dfTemp])\n",
    "df.to_csv(\"grouped.csv\", sep = \";\")"
   ]
